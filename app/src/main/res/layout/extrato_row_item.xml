<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="45dp"
    android:layout_width="match_parent">

    <TextView
        android:id="@+id/textview_extrato_data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textview_extrato_descricao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/textview_extrato_data"
        android:layout_marginLeft="34dp"
        app:layout_constraintTop_toTopOf="@+id/textview_extrato_data"
        android:layout_marginTop="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/textview_extrato_data"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textview_extrato_valor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginRight="6dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="13dp"
        app:layout_constraintLeft_toRightOf="@+id/textview_extrato_descricao"
        android:layout_marginLeft="8dp"
        app:layout_constraintHorizontal_bias="0.939" />
</android.support.constraint.ConstraintLayout>
